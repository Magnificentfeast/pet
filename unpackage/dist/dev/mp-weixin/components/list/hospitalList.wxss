.hospital-item {
  padding: 32rpx 30rpx 36rpx;
  border-bottom: 2rpx solid #F4F4F4;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}
.hospital-item .head-img {
  width: 125rpx;
  height: 125rpx;
  -webkit-box-shadow: 0px -5px 10px 0px rgba(119, 119, 119, 0.2);
          box-shadow: 0px -5px 10px 0px rgba(119, 119, 119, 0.2);
  -webkit-border-radius: 20rpx;
          border-radius: 20rpx;
  overflow: hidden;
  display: inline-block;
  position: relative;
}
.hospital-item .head-img image {
  width: 125rpx;
  height: 125rpx;
}
.hospital-item .head-img .collect {
  position: absolute;
  top: 0;
  left: 0;
  width: 87rpx;
  height: 107rpx;
  z-index: 1;
}
.hospital-item .infos {
  display: inline-block;
  margin-left: 27rpx;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
}
.hospital-item .infos .name {
  font-size: 35rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 500;
  color: #000000;
  line-height: 38rpx;
  width: 470rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  height: 38rpx;
  margin-bottom: 15rpx;
}
.hospital-item .infos .location {
  font-size: 25rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  color: #555555;
  line-height: 38rpx;
}
.hospital-item .infos .info .star-box {
  display: inline-block;
}
.hospital-item .infos .info .star-box .star-item {
  display: inline-block;
}
.hospital-item .infos .info .star-box .star-item .star {
  width: 20rpx;
  height: 20rpx;
  display: inline-block;
  margin-right: 9rpx;
}
.hospital-item .infos .info .mark {
  font-size: 23rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  color: #FE6545;
  line-height: 44rpx;
}
.hospital-item .infos .info .range {
  float: right;
  font-size: 25rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  color: #555555;
  line-height: 38rpx;
}
.hospital-item .show-type {
  width: 250rpx;
  height: 50rpx;
  -webkit-border-radius: 50rpx;
          border-radius: 50rpx;
  font-size: 25rpx;
}
.hospital-item .show-type .type {
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  line-height: 50rpx;
  margin-left: 58rpx;
}
.hospital-item .show-type .per {
  font-family: DIN;
  line-height: 50rpx;
  margin-left: 23rpx;
}
.hospital-item .show-type .per .num {
  font-size: 30rpx;
  font-weight: bold;
}
.hospital-item .show-type.type1 {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  color: #FFFFFF;
}
.hospital-item .show-type.type2 {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  color: #777777;
}

