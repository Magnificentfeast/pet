.addpet {
  padding-bottom: 54rpx;
}
.addpet .head {
  position: relative;
}
.addpet .head .bg {
  height: 600rpx;
  width: 100%;
  z-index: -1;
}
.addpet .head .img {
  position: absolute;
  width: 200rpx;
  height: 200rpx;
  left: 50%;
  bottom: 79rpx;
  margin-left: -100rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
}
.addpet .head .upload {
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  left: 50%;
  bottom: 79rpx;
  margin-left: -100rpx;
  -webkit-border-radius: 50%;
          border-radius: 50%;
  background-color: #FFFFFF;
}
.addpet .head .upload .icon {
  width: 78rpx;
  height: 68rpx;
  margin: 45rpx 60rpx 0;
}
.addpet .head .upload .text {
  margin-top: 10rpx;
  font-size: 20rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #999999;
  text-align: center;
}
.addpet .cell-gruops {
  font-size: 30rpx;
  background: #FFFFFF;
  -webkit-box-shadow: 0rpx 15rpx 15rpx 0px rgba(119, 119, 119, 0.05);
          box-shadow: 0rpx 15rpx 15rpx 0px rgba(119, 119, 119, 0.05);
  -webkit-border-radius: 30rpx;
          border-radius: 30rpx;
  overflow: hidden;
  margin: 0 40rpx 0;
}
.addpet .cell-gruops .u-form-item {
  padding: 0 28rpx;
}
.addpet .cell-gruops .select {
  margin-bottom: 10rpx;
}
.addpet .cell-gruops .select .select-item {
  width: 275rpx;
  height: 60rpx;
  background: #D7D7D7;
  -webkit-border-radius: 10rpx;
          border-radius: 10rpx;
  display: inline-block;
  line-height: 60rpx;
  text-align: center;
}
.addpet .cell-gruops .select .select-item:last-child {
  margin-left: 60rpx;
}
.addpet .cell-gruops .select .checked {
  background: url(data:image/png;base64,) no-repeat;
  background-size: 100%;
  color: #FFFFFF;
}
.addpet .base {
  position: relative;
  top: -40rpx;
}
.addpet .phone {
  margin-top: 40rpx;
}
.addpet .phone .title {
  font-size: 30rpx;
  font-family: Microsoft YaHei;
  font-weight: 400;
  color: #313131;
  line-height: 100rpx;
  padding: 0 28rpx;
}
.addpet .phone .phone-item {
  color: #555555;
}
.addpet .phone .phone-item .img-box {
  position: relative;
  border: 2rpx solid #DDDDDD;
  -webkit-border-radius: 15rpx;
          border-radius: 15rpx;
  height: 253rpx;
  width: 100%;
  margin-bottom: 24rpx;
}
.addpet .phone .phone-item .img-box .icon {
  width: 64rpx;
  height: 56rpx;
  margin: 95rpx 63rpx 0;
}
.addpet .phone .phone-item .img-box video {
  width: 100%;
  height: 100%;
}
.addpet .phone .phone-item .img-box .u-clear-icon {
  position: absolute;
  top: -20rpx;
  right: -20rpx;
}
.addpet .phone .phone-item .text {
  text-align: center;
  margin-bottom: 35rpx;
  font-size: 30rpx;
  font-family: Alibaba PuHuiTi;
  font-weight: 400;
  color: #555555;
}
.addpet .btn {
  height: 100rpx;
  -webkit-border-radius: 50rpx;
          border-radius: 50rpx;
  line-height: 100rpx;
  font-size: 35rpx;
  text-align: center;
}
.addpet .submit {
  background-color: #18abff;
  color: #FFFFFF;
  margin: 88rpx 25rpx 0;
}
.addpet .zancun {
  margin: 34rpx 200rpx 0;
  border: 2px solid #DDDDDD;
  color: #777777;
}

